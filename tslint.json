{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "array-type": false,
    "interface-name": false,
    "interface-over-type-literal": false,
    "member-access": false,
    "max-classes-per-file": false
  }
}
